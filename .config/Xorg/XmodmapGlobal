! Remove the modifiers to assign a new action without the old modifiers
remove Lock = Caps_Lock
remove Mod1 = Alt_L 
remove Mod4 = Super_L Super_R
remove Mod5 = ISO_Level3_Shift
remove control = Control_R Control_L
! Assign the code of the key (left) to the action we want
!!keysym Escape = Alt_L
!!keysym Caps_Lock = Escape
!!keysym Alt_L = ISO_Level3_Shift
!!keysym Menu = Super_R
! Todas las teclas que tengan Alt_L como acci√≥n se les asigna 
! el modificador Mod1
! Add back tho modifiers
add Mod1 = Alt_L 
add Mod4 = Super_L Super_R
add Mod5 = ISO_Level3_Shift
add control = Control_R Control_L

! Tick with altgr+t
keycode  45 = t T t T U2714 THORN U2714
